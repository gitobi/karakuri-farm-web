{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "flowtype"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": ["node_modules", "/"]
        }
      },
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true,
      }
    }
}
